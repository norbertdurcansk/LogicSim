IN:LEFT_GEAR
IN:RIGHT_GEAR
IN:PILOT_CABIN
OUT:RED_LED
OUT:GREEN_LED


OR1:type:OR
OR1:delay:0
.INPUT:OR1:PINLIST:LEFT_GEAR:RIGHT_GEAR
.OUTPUT:OR1:PINLIST:OR1_OUT

OR2:type:OR
OR2:delay:0
.INPUT:OR2:PINLIST:PILOT_CABIN:OR1_OUT
.OUTPUT:OR2:PINLIST:RED_LED_FLAG

NOT1:type:NOT
NOT1:delay:0
.INPUT:NOT1:PINLIST:RED_LED_FLAG
.OUTPUT:NOT1:PINLIST:RED_LED



NOT_LEFT_GEAR:type:NOT
NOT_LEFT_GEAR:delay:0
.INPUT:NOT_LEFT_GEAR:PINLIST:LEFT_GEAR
.OUTPUT:NOT_LEFT_GEAR:PINLIST:LEFT_GEAR'



NOT_RIGHT_GEAR:type:NOT
NOT_RIGHT_GEAR:delay:0
.INPUT:NOT_RIGHT_GEAR:PINLIST:RIGHT_GEAR
.OUTPUT:NOT_RIGHT_GEAR:PINLIST:RIGHT_GEAR'


NOT_PILOT_CABIN:type:NOT
NOT_PILOT_CABIN:delay:0
.INPUT:NOT_PILOT_CABIN:PINLIST:PILOT_CABIN
.OUTPUT:NOT_PILOT_CABIN:PINLIST:PILOT_CABIN'

GREEN1:type:AND
GREEN1:delay:0
.INPUT:GREEN1:PINLIST:PILOT_CABIN':LEFT_GEAR'
.OUTPUT:GREEN1:PINLIST:GREEN1_OUT

GREEN:type:AND
GREEN:delay:0
.INPUT:GREEN:PINLIST:GREEN1_OUT:RIGHT_GEAR'
.OUTPUT:GREEN:PINLIST:GREEN_LED



